
uniform sampler2D MyTex;
uniform mat4 proj;

struct VS_OUTPUT {
  vec4 position;
  vec2 texcoord;
};

VS_OUTPUT vs_main(vec4 position,  vec2 texcoord) {
  VS_OUTPUT o;
  o.position = position * proj;
  o.texcoord = texcoord;
  return o;
}

vec4 ps_main(VS_OUTPUT o) {
  return texture2D(MyTex, o.texcoord);
}

