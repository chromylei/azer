
uniform mat4 wvp;
uniform mat4 world;
uniform sampler2D MyTex;

struct VsOutput {
  vec4 __afx_position;
  vec2 texcoord;
  vec4 normal;
};

VsOutput vs_main(vec3 position, vec2 texcoord, vec3 normal) {
  VsOutput o;
  o.__afx_position = vec4(position, 1) * wvp;
  o.texcoord = texcoord;
  o.normal = vec4(normal, 1) * world;
  return o;
}

vec4 ps_main(VsOutput o) {
  return texture2D(MyTex, o.texcoord);
}

